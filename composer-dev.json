{
    "name": "nowise/uup-application",
    "description": "Run same monitored action both from CLI and HTTP requests.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Anders Lövgren",
            "email": "andlov@nowise.se"
        },
        {
            "name": "Anders Lövgren",
            "email": "anders.lovgren@interactivesolutions.se"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../uup-application-options"
        }
    ],
    "require": {
        "php": ">=7.4",
        "nowise/uup-application-options": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "UUP\\Application\\": "source/Application",
            "UUP\\Example\\Application\\": "example"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UUP\\Tests\\Application\\": "tests/Application"
        }
    }
}
